{
  "include": ["src"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "lib": ["dom", "es2015", "es2016", "esnext"],
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
